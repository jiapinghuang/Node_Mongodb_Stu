<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
		<link rel="stylesheet" href="./css/main.css" />
	</head>
	<body>
		<table>
		<tr>
			<td>姓名</td>
			<td>年龄</td>
			<td>性别</td>
			<td>邮箱</td>
			<td>爱好</td>
			<td>入学日期</td>
			<td>所学专业</td>
		</tr>
			{{each Stus}}
				<tr>
					<td>{{$value.name}}</td>
					<td>{{$value.age}}</td>
					<td>{{$value.sex=='0' ? '男':'女'}}</td>
					<td>{{$value.email}}</td>
					<td>
						{{each $value.hobby}}
							<span>{{$value}}</span>						
						{{/each}}
					</td>
					<td>{{dateformat($value.enterDate,'yyyy-mm')}}</td>
					<td>{{$value.college}}</td>
				</tr>
			{{/each}}
			
		</table>
	</body>
</html>
